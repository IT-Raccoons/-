#include <iostream>
#include <vector> //динамический массив, то же самое, что и new
using namespace std;

int main()
{
  vector<int> a; // создание пустого массива
  vector<double> b(10); // создание массива размера 10
  vector<int> c(10, 5); // создание массива из 10 элементов, запроненный "5"
  a.push_back(10); // добавление элемента "10" в конец
  a.begin(); // возврат указателя на начало массива
  a.end(); // возврат указателя на элемент после массива
  a.insert(a.end(), 10); // вставка "10" в конец массива
  a.insert(a.end(), 5, 10); // вставка 5 "10" в конец массива
  a.insert(a.begin(), c.begin(), c.end()); // вставка в начало массива всех эл-тов массива c
  a.size(); // возврат размера массива
  a.resize(10); //  изменение размера массива
  a.resize(20, 8); // изменение размера массива и заполнение новых элементов "8"
  a.assign(25, 0); // изменение размера массива и заполнение всех элементов "0"
  a.clear(); // очищение массива (размер массива становится равным 0)
  a.erase(a.begin() + 1); // удаление 2-го элемента
  return 0;
}




