# Невыполненные задания
Репозиторий команды для невыполненных заданий.
#include <iostream> 
#include <string>
using namespace std;
int main() {
	int n, m;
	cin >> n >> m;
	long long t[n];
	cin.get();
	string k[n], a[n];
	for (int i = 0; i < n; i++) {
		cin >> t[i];
		getline(cin, k[i]);
		getline(cin, a[i]);
	}
	long long p[m];
	for (int i = 0; i < m; i++) {
		cin >> p[i];
	}
	for (int i = 0; i < m; i++) {
		for (int j = 0; j < n; j++) {
			if (t[j] == p[i]) {
				cout << k[j] << " (" << a[j] << ")" << endl;
			}
		}
	}
	return 0;
}
