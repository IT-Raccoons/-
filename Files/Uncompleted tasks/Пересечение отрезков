#include <iostream>
using namespace std;

int main()
{
    int x1,x2,x3,x4,y1,y2,y3,y4;
    float k1,k2;
    cin>>x1>>y1>>x2>>y2>>x3>>y3>>x4>>y4;
    if (x1>x2)
    {
   		    swap(x1,x2);
            swap(y1,y2);
    	}
    if (x3>x4)
    {
   		    swap(x3,x4);
            swap(y3,y4);
  	    }
    k1=(y2-y1)/(x2-x1);
    k2=(y4-y3)/(x4-x3);
    if(k1!=k2)
	{
        if ((k1*x1+y1-k1*x1)==(k2*x2+y3-k2*x3))
       	    cout<<"Yes";
	    else 
            cout<<"No";
       }
   	else cout<<"No";
}
