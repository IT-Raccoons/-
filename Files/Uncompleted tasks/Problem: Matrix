#include "pch.h"
#include <iostream>
using namespace std;

void movelines(double **matrix, int size, int i, unsigned int **c_o_m) {
	for (int q = 0; q < size; q++) {
		if (matrix[q][i] < matrix[i][i]) {

		}
	}
	c_o_m++;
}

int main() {
	// Узнаем размеры матрицы (по условию - квадратной)
	int size;
	cin >> size;
	// Инициализируем матрицу
	double** matrix = new double*[size];
	for (int i = 0; i < size; i++) {
		matrix[i] = new double[size];
	}
	// Заполняем матрицу
	for (int i = 0; i < size; i++) {
		for (int q = 0; q < size; q++) {
			cin >> matrix[i][q];
		}
	}
	// Инициализируем счетчик перестановок строк
	unsigned int count_of_movements = 0;
	// Находим наибольшую ненулевую строку
	for (int i = 0; i < size; i++) {
		movelines(matrix, size, i, count_of_movements);
	}







	cout << count_of_movements << endl;
	// Вывод матрицы
	for (int q = 0; q < size; q++) {
		for (int w = 0; w < size; w++) {
			cout << matrix[q][w] << " ";
		}
		cout << endl;
	}

	delete[] *matrix;
	return 0;
}

