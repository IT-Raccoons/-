Матрица смежности
#include <iostream>
using namespace std;
 
int main()
{
    int n, c = 0;
    cin >> n;
    int matrix[n][n];
 
    for (int i = 0; i < n; i++)
        for (int q = 0; q < n; q++)
        {
            cin >> matrix[i][q];
            if (matrix[i][q])
                c++;    
        }
 
    cout << n << " " << c << endl;
     
    for (int i = 0; i < n; i++)
        for (int q = 0; q < n; q++)
        {
            if (matrix[i][q])
                cout << i+1 << " " << q+1 << endl;  
        }
 
    return 0;
}

Ориентированный граф
#include <iostream>
#include <vector>
using namespace std;
 
int main()
{
    int n, not_equal = 0, loop = 0;
    cin >> n;
    vector<vector <int>> matrix (n, vector<int> (n, 0));
 
    for (int i = 0; i < n; i++)
        for (int q = 0; q < n; q++)
            cin >> matrix[i][q];
 
    for (int i = 0; i < n; i++)
        for (int q = 0; q < n; q++)
        {
            if (i != q && matrix[i][q] != matrix[q][i])
                not_equal++;
            else if (i == q && matrix[i][q] == 1)
                loop++;
        }
    if (not_equal != 0 && loop == 0)
        cout << "YES";
    else
        cout << "NO";
    return 0;
}
