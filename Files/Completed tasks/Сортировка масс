#include <iostream>
#include <cmath>
#include <string>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    int n;
    cin>>n;
    int Massa[n], Index[n];
    string EdIzm[n];
    double forSort[n];
    for(int i = 0; i < n; i++ )
    {
      cin>>Massa[i]>>EdIzm[i];
      Index[i]=i;
      forSort[i]=Massa[i];
      if(EdIzm[i].size()==1 )
      {
        if (EdIzm[i] == "p" )
              {
                forSort[i] *= 16380;
            }
        if(EdIzm[i] == "t")
             {
            forSort[i] *= pow(10,6);
          }
        }
        else if(EdIzm[i].size()==2 )
        {
            if(EdIzm[i].at(0)  == 'm' )
            {
                forSort[i] /= ( 10*10*10 );
             }
            else if(EdIzm[i].at(0)  == 'k')
              {
                forSort[i] *= ( 10*10*10 );
              }
              else if(EdIzm[i].at(0) == 'M' )
              {
                forSort[i] *= pow (10, 6);
              }
              else if(EdIzm[i].at(0) == 'G')
             {
                forSort[i] *= pow ( 10,9 );
             }
             if (EdIzm[i].at(1) == 'p' )
              {
                forSort[i] *= 16380;
            }
            if(EdIzm[i].at(1) == 't')
             {
                forSort[i] *= pow(10,6);
          }
        }
    }
     for(int i=0; i < n-1; i++)
    {
        for(int j=i+1; j<n; j++)
        {
            if (forSort[i]>forSort[j])
            {
                swap(forSort[i], forSort[j]);
                swap(Index[i], Index[j]);
            }
        }
    }
     for(int i=0; i < n-1; i++)
    {
        for(int j=i+1; j<n; j++)
        {
            if(forSort[i]==forSort[j] && Index[i]>Index[j] && i<j)
            {
                swap(Index[i], Index[j]);
            }
        }
    }
     
    for(int i = 0; i < n; i++ )
    { 
        cout<<Massa[Index[i]]<<" "<<EdIzm[Index[i]]<<endl;
    }
return 0;
}


