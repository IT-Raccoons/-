#include <iostream>
#include <string>
#include <stack>
using namespace std;

int main()
{
	int tmp, tmp_del;
	string s;
	stack<int> numbers;
	getline(cin, s);
	for (int i = 0; i < s.size(); i++)
		//÷èñëà
		if (i == 0 && s[i] != ' ')
			numbers.push(int(s[i] - '0'));
		else if (i != 0 && int(s[i] - '0') >= 0 && int(s[i] - '0') <= 9 && s[i-1] == ' ')	
			numbers.push(int(s[i] - '0'));
		else if (i != 0 && int(s[i] - '0') >= 0 && int(s[i] - '0') <= 9 && s[i-1] != ' ')
		{
			tmp = numbers.top() * 10;
			numbers.pop();
			tmp += int(s[i] - '0');
			numbers.push(tmp);
		}
		
		//ñèìâîëû
		else if (s[i] == '+' || s[i] == '-' || s[i] == '*' || s[i] == '/')
			if (numbers.size() > 1)
			{
				if (s[i] == '+')
				{
					tmp = numbers.top();
					numbers.pop();
					tmp += numbers.top();
					numbers.pop();
					numbers.push(tmp);
				}
				else if (s[i] == '-')
				{
					tmp = numbers.top();
					numbers.pop();
					tmp -= numbers.top();
					numbers.pop();
					numbers.push(tmp * -1);
				}
				else if (s[i] == '*')
				{
					tmp = numbers.top();
					numbers.pop();
					tmp *= numbers.top();
					numbers.pop();
					numbers.push(tmp);
				}
				else if (s[i] == '/')
				{
					tmp_del = numbers.top();
					numbers.pop();
					tmp = numbers.top();
					numbers.pop();
					numbers.push(tmp / tmp_del);
				}
			}
			else
			{
				cout << -1;
				return -1;
			}
	if (numbers.size() == 1)
		cout << numbers.top();
	else
	{
		cout << -1;
		return -1;
	}
	return 0;
}
